   "source": [
    "import torch\n",
    "\n",
    "model = torch.hub.load('ultralytics/yolov5', 'yolov5s')\n",
    "\n",
    "def detect_objects(image_path):\n",
    "    results = model(image_path)\n",
    "    results.show()\n",
    "\n",
    "image_path = 'path_to_image.jpg'  # Replace with your image path\n",
    "detect_objects(image_path)"
   ]
